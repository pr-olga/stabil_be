{% extends 'base.html.twig' %}
{% block body %}
<div class="container">
<h1 class="mt-5">Hello <b>{{ admin.username }}</b>!</h1>
<div class="row">
  <div class="col-md-6">
    <table class="table  mt-5">
      <tbody>
          <tr>
            <td><b>ID</b></td>
            <td>{{ admin.id}}</td>
          </tr>
          <tr>
            <td><b>Name</b></td>
            <td>{{ admin.username}}</td>
          </tr>
          <tr>
            <td><b>E-Mail</b></td>
            <td>{{ admin.email}}</td>
          </tr>
          <tr>
            <td><b>Password</b></td>
            <td>{{ admin.password}}</td>
          </tr>
          <tr>
            <td><b>Api Key</b></td>
            <td>{{ admin.apikey}}</td>
          </tr>
          <tr>
            <td><b>Account since</b></td>
            <td>{{ dump(admin.createdat) }}</td>
          </tr>
      </tbody>
    </table>
   {#  <button type="button" class="btn btn-success mr-2 float-right">Modify</button> #}
  </div>
</div>
</div>
{% endblock %}
